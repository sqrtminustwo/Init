{
    "files_root": [
        "c++",
        "default"
    ],
    "structure": {
        "default": {
            "dirs": [
                [
                    "src"
                ],
                [
                    "include"
                ]
            ],
            "files": [
                {
                    "name": "^.*\\.cpp$",
                    "src_path": [
                        "src"
                    ],
                    "dst_path": [
                        "src"
                    ]
                },
                {
                    "name": "^CMakePresets\\.json$",
                    "src_path": [
                        "configs"
                    ],
                    "dst_path": []
                },
                {
                    "name": "^CMakeLists\\.txt$",
                    "src_path": [
                        "configs"
                    ],
                    "dst_path": []
                },
                {
                    "name": "^\\.clangd$",
                    "src_path": [
                        "clang"
                    ],
                    "dst_path": []
                }
            ]
        },
        "github": {
            "files": [
                {
                    "name": "^.+$",
                    "src_path": [
                        "github"
                    ],
                    "dst_path": []
                }
            ]
        }
    }
}
